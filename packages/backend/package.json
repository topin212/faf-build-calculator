{
  "name": "backend",
  "packageManager": "yarn@3.5.0",
  "scripts": {
    "build": "echo 'Building backend' && yarn webpack --config ./webpack.config.js",
    "run": "yarn ts-node src/index.ts",
    "invoke-local": "yarn serverless invoke local -f helloworld",
    "package": "yarn serverless package",
    "deploy": "yarn serverless deploy"
  },
  "private": true,
  "dependencies": {
    "library": "workspace:^"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.114",
    "@types/node": "^18.15.11",
    "@types/serverless": "^3",
    "@types/webpack-node-externals": "^3",
    "aws-lambda": "^1.0.7",
    "pnp-webpack-plugin": "^1.7.0",
    "serverless": "^3.29.0",
    "serverless-offline": "^12.0.4",
    "serverless-plugin-optimize": "^4.2.1-rc.1",
    "serverless-plugin-typescript": "^2.1.4",
    "serverless-webpack": "^5.11.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "webpack": "^5.77.0",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^3.0.0"
  }
}
